# Snowflake Configuration
SNOWFLAKE_ACCOUNT=your_account.region
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_DATABASE=ATS_DB
SNOWFLAKE_SCHEMA=ATS_SCHEMA
SNOWFLAKE_ROLE=PUBLIC

# Snowflake Private Key (for Kafka Connector - use key-pair authentication)
# Generate using: openssl genrsa 2048 | openssl pkcs8 -topk8 -inform PEM -out rsa_key.p8 -nocrypt
SNOWFLAKE_PRIVATE_KEY=

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=kafka:9092
